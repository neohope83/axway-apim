pipeline {
   agent any



   stages {
      stage('Build') {
         steps {
            // Get the repository from GitHub
            git 'https://github.com/neohope83/axway-apim.git'

            // Run API-cli 
           
            sh "/opt/apim-cli/scripts/apim.sh api import -c /sample-api-project/api-config.json"
         }
      }
   }
}
